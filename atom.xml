---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Abhi's Experiments</title>
 <link href="http://abhidsm.github.com/"/>
 <link type="application/atom+xml" rel="self" href="http://abhidsm.com/atom.xml"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://abhidsm.com/</id>
 <author>
   <name>Abhilash M A</name>
   <email>abhidsm@gmail.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <id>http://abhidsm.com{{ post.id }}</id>
   <link href="http://abhidsm.com{{ post.url }}"/>
   <title>{{ post.title }}</title>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <author>
     <name>Abhilash M A</name>
     <email>abhidsm@gmail.com</email>
   </author>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
